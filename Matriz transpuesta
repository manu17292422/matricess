#include <iostream>
using namespace std;
int main()
{  int num[3][3];
for(int i=0;i<3;i++)
{
    for(int j=0;j<3;j++)
        {
            cout<<"Digite el numero: ["<<i<<"]["<<j<<"]";
            cin>>num[i][j];
        }
}
cout<<"La matriz normal: "<<endl;
for(int i=0;i<3;i++)
{
    for(int j=0;j<3;j++)
    {
        cout<<num[i][j];
    }
    cout<<endl;
}
cout<<"matriz tranzpuesta: "<<endl;
for(int i=0;i<3;i++)
{
    for(int j=0;j<3;j++)
        {
            cout<<num[j][i];
        }
        cout<<endl;
}
return 0;

}
