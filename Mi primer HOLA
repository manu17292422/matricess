#include <iostream>
using namespace std;
int main()
{  int num[7][7];
for(int i=0;i<7;i++)
{
    for(int j=0;j<7;j++)
    {
        num[i][j]=1;
    }}
for(int i=0;i<7;i++)

{
    for(int j=0;j<7;j++)
    {
        if(j==0||j==6||i==3)
        {
            cout<<"*";
        }
        else {cout<<" ";}

}cout<<endl;}
for(int i=0;i<7;i++)
{
    for(int j=0;j<7;j++)
    {
        if(j==0||i==0||j==6||i==6)
        {
            cout<<"*";

        }
        else{ cout<<" ";}
    }
    cout<<endl;

}
for(int i=0;i<7;i++)
{
    for(int j=0;j<7;j++)
    {
        if(j==0||i==6)
        {
            cout<<"*";
        }
        else
        {
            cout<<" ";
        }
    }
    cout<<endl;
}
for(int i=0;i<7;i++)
{
    for(int j=0;j<7;j++)
    {
        if(j==0||i==0||j==6||i==3)
        {
            cout<<"*";
        }
        else{cout<<" ";}
    }
    cout<<endl;
}
return 0;

}
